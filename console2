using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApp2
{
    class Program
    {
        static void Main(string[] args)
        {
            int time = 50;
            if (time<18)
            {
                Console.WriteLine("good moring");
            }
            else
            {
                Console.WriteLine("good night");
            }
            Console.ReadLine();
        }
    }
}
